-- Extras

-- Teleport to player by name (case insensitive, partial)
sectionExtras:NewTextBox("Teleport To Player", "Oyuncu ismi yaz", function(input)
    if input == "" then return end
    local target = nil
    local linput = input:lower()
    for _,plr in pairs(Players:GetPlayers()) do
        if plr.Name:lower():find(linput) then
            target = plr
            break
        end
    end
    if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
        rootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(0,5,0)
    else
        warn("Oyuncu bulunamadı veya karakter hazır değil!")
    end
end)

-- Teleport dropdown (liste ile seç)
local playerNames = {}
local function refreshPlayerNames()
    playerNames = {}
    for _,plr in pairs(Players:GetPlayers()) do
        table.insert(playerNames, plr.Name)
    end
end
refreshPlayerNames()

sectionExtras:NewDropdown("Teleport To Player (Select)", "Listeden oyuncu seç", playerNames, function(selected)
    local plr = Players:FindFirstChild(selected)
    if plr and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
        rootPart.CFrame = plr.Character.HumanoidRootPart.CFrame + Vector3.new(0,5,0)
    else
        warn("Oyuncu bulunamadı veya hazır değil!")
    end
end)

Players.PlayerAdded:Connect(refreshPlayerNames)
Players.PlayerRemoving:Connect(refreshPlayerNames)

-- Auto Rejoin toggle
local autoRejoinConn
local autoRejoinEnabled = false
sectionExtras:NewToggle("Auto Rejoin", "Sunucudan atılırsan tekrar bağlan", function(state)
    autoRejoinEnabled = state
    if state then
        if autoRejoinConn then autoRejoinConn:Disconnect() end
        autoRejoinConn = LocalPlayer.OnTeleport:Connect(function()
            if autoRejoinEnabled then
                TeleportService:Teleport(game.PlaceId, LocalPlayer)
            end
        end)
    else
        if autoRejoinConn then autoRejoinConn:Disconnect() end
    end
end)

-- Anti AFK toggle
local antiAfkConn
local antiAfkEnabled = false
sectionExtras:NewToggle("Anti AFK", "Atılmanı engeller", function(state)
    antiAfkEnabled = state
    if state then
        antiAfkConn = game:GetService("VirtualUser")
        antiAfkConn:Button2Down(Vector2.new(0,0), Workspace.CurrentCamera.CFrame)
        antiAfkConn:Button2Up(Vector2.new(0,0), Workspace.CurrentCamera.CFrame)
    else
        antiAfkConn = nil
    end
end)

-- Super Punch button
sectionExtras:NewButton("Super Punch", "Yakındaki oyuncuları hızlı vur", function()
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
            local dist = (plr.Character.HumanoidRootPart.Position - rootPart.Position).Magnitude
            if dist < 10 then
                local hitbox = Instance.new("Part")
                hitbox.Size = Vector3.new(10,10,10)
                hitbox.Transparency = 1
                hitbox.CanCollide = false
                hitbox.Anchored = true
                hitbox.CFrame = plr.Character.HumanoidRootPart.CFrame
                hitbox.Parent = Workspace
                game.Debris:AddItem(hitbox, 0.1)
            end
        end
    end
end)

-- Speed Aura toggle
local speedAuraConn
local speedAuraEnabled = false
sectionExtras:NewToggle("Speed Aura", "Yakındaki oyuncuları yavaşlat", function(state)
    speedAuraEnabled = state
    if speedAuraEnabled then
        speedAuraConn = RunService.Heartbeat:Connect(function()
            for _,plr in pairs(Players:GetPlayers()) do
                if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                    local dist = (plr.Character.HumanoidRootPart.Position - rootPart.Position).Magnitude
                    local hum = plr.Character:FindFirstChild("Humanoid")
                    if dist < 15 and hum then
                        hum.WalkSpeed = 5
                    elseif hum then
                        hum.WalkSpeed = 16
                    end
                end
            end
        end)
    else
        if speedAuraConn then speedAuraConn:Disconnect() end
        for _,plr in pairs(Players:GetPlayers()) do
            if plr.Character then
                local hum = plr.Character:FindFirstChild("Humanoid")
                if hum then
                    hum.WalkSpeed = 16
                end
            end
        end
    end
end)

-- Infinite Jump toggle
