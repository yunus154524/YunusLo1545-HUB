-- Chat spam durumunu tutan değişken
local spamEnabled = false
local spamMessages = {
    "Solara rocks!", "I hacked this game!", "Epic Script!", "Join my Discord!", "Follow me!", "Best GUI ever!"
}
local spamConn = nil

-- Servisleri alıyoruz
local RunService = game:GetService("RunService")
local Players = game:GetService("Players")
local TextChatService = game:GetService("TextChatService")

-- Kullanıcının chat spam'i açıp kapatmasını sağlayan GUI öğesi
sectionExtras:NewToggle("Chat Spam", "Otomatik chat spam (kapalı tavsiye edilir)", function(state)
    spamEnabled = state
    if spamEnabled then
        spamConn = RunService.Heartbeat:Connect(function()
            if TextChatService.ChatVersion == Enum.TextChatVersion.LegacyChatService then
                -- Eski chat sistemi için
                local defaultChatEvents = game:GetService("ReplicatedStorage"):FindFirstChild("DefaultChatSystemChatEvents")
                if defaultChatEvents then
                    local sayMessageRequest = defaultChatEvents:FindFirstChild("SayMessageRequest")
                    if sayMessageRequest then
                        sayMessageRequest:FireServer(spamMessages[math.random(1, #spamMessages)], "All")
                    end
                end
            elseif TextChatService.ChatVersion == Enum.TextChatVersion.TextChatService then
                -- Yeni chat sistemi için
                local defaultChannel = TextChatService:FindFirstChild("TextChannels"):FindFirstChild("RBXGeneral")
                if defaultChannel then
                    defaultChannel:SendAsync(spamMessages[math.random(1, #spamMessages)])
                end
            end
        end)
    elseif spamConn then
        spamConn:Disconnect()
        spamConn = nil -- Bağlantıyı kestikten sonra değişkeni temizliyoruz
    end
end)

-- Teleport to mouse toggle ve T tuşu ışınlama sistemi
local teleportToMouseEnabled = false
sectionExtras:NewToggle("Teleport to Mouse (T toggle)", "T ile imleç pozisyonuna ışınlan", function(state)
    teleportToMouseEnabled = state
end)

UserInputService.InputBegan:Connect(function(input, gp)
    if gp then return end
    if input.KeyCode == Enum.KeyCode.T and teleportToMouseEnabled then
        local mouseLoc = UserInputService:GetMouseLocation()
        local ray = Workspace.CurrentCamera:ScreenPointToRay(mouseLoc.X, mouseLoc.Y)
        local pos = ray.Origin + ray.Direction * 200
        if rootPart then
            rootPart.CFrame = CFrame.new(pos)
        end
    end
end)

