-- Yeni Eklenen Tab: Teleport Tools
local tabTeleport = Window:CreateTab("Teleport Tools", nil)
tabTeleport:CreateSection("Teleport Features")
tabTeleport:CreateButton({
	Name = "Teleport to Spawn",
	Callback = function()
		if Workspace:FindFirstChild("SpawnLocation") then
			rootPart.CFrame = Workspace.SpawnLocation.CFrame + Vector3.new(0,5,0)
		else
			warn("SpawnLocation bulunamadı!")
		end
	end
})

tabTeleport:CreateButton({
	Name = "Teleport to Random Player",
	Callback = function()
		local players = Players:GetPlayers()
		local randomPlr = players[math.random(1, #players)]
		if randomPlr ~= LocalPlayer and randomPlr.Character and randomPlr.Character:FindFirstChild("HumanoidRootPart") then
			rootPart.CFrame = randomPlr.Character.HumanoidRootPart.CFrame + Vector3.new(0,5,0)
		end
	end
})

tabTeleport:CreateButton({
	Name = "Teleport to Safe Height",
	Callback = function()
		rootPart.CFrame = rootPart.CFrame + Vector3.new(0, 1000, 0)
	end
})

tabTeleport:CreateSlider({
	Name = "Teleport Distance",
	Range = {5, 100},
	Increment = 1,
	Suffix = "Studs",
	CurrentValue = 5,
	Flag = "TeleportDist",
	Callback = function(Value)
		rootPart.CFrame = rootPart.CFrame + rootPart.CFrame.LookVector * Value
	end
})

