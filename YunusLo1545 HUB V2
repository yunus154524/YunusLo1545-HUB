tabVisuals:CreateSlider({
	Name = "Field of View (FOV)",
	Range = {70, 120},
	Increment = 1,
	Suffix = "FOV",
	CurrentValue = 70,
	Flag = "FOVSlider",
	Callback = function(Value)
		Workspace.CurrentCamera.FieldOfView = Value
	end
})

tabVisuals:CreateToggle({
	Name = "Fullbright",
	CurrentValue = false,
	Flag = "Fullbright",
	Callback = function(Value)
		fullbrightEnabled = Value
    if Value then
        Lighting.Brightness = 2
        Lighting.FogEnd = 999999
        Lighting.GlobalShadows = false
    else
        Lighting.Brightness = originalBrightness
        Lighting.FogEnd = originalFogEnd
        Lighting.GlobalShadows = originalGlobalShadows
    end
	end
})
local originalBrightness = Lighting.Brightness
local originalFogEnd = Lighting.FogEnd
local originalGlobalShadows = Lighting.GlobalShadows

tabVisuals:CreateToggle({
	Name = "Xray (Wallhack)",
	CurrentValue = false,
	Flag = "Xray",
	Callback = function(Value)
		xrayEnabled = Value
    for _, part in pairs(Workspace:GetDescendants()) do
        if part:IsA("BasePart") and not part.Parent:IsA("Model") or not part.Parent:FindFirstChild("Humanoid") then
            part.Transparency = Value and 0.7 or 0
        end
    end
	end
})

tabVisuals:CreateToggle({
	Name = "Tracers",
	CurrentValue = false,
	Flag = "Tracers",
	Callback = function(Value)
		tracersEnabled = Value
    updateTracers()
	end
})

local tracersEnabled = false
local tracerLines = {}
local function updateTracers()
    for _, line in pairs(tracerLines) do
        if line then line:Destroy() end
    end
    tracerLines = {}
    if tracersEnabled then
        for _, plr in pairs(Players:GetPlayers()) do
            if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                local line = Instance.new("LineHandleAdornment")
                line.Adornee = plr.Character.HumanoidRootPart
                line.Length = 100
                line.Thickness = 1
                line.Color3 = Color3.new(1, 0, 0)
                line.AlwaysOnTop = true
                line.Parent = CoreGui
                table.insert(tracerLines, line)
            end
        end
    end
end
Players.PlayerAdded:Connect(updateTracers)
Players.PlayerRemoving:Connect(updateTracers)

tabVisuals:CreateToggle({
	Name = "Chams",
	CurrentValue = false,
	Flag = "Chams",
	Callback = function(Value)
		chamsEnabled = Value
    for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer and plr.Character then
            for _, part in pairs(plr.Character:GetChildren()) do
                if part:IsA("BasePart") then
                    part.Material = Value and Enum.Material.ForceField or Enum.Material.SmoothPlastic
                    part.Color = Value and Color3.new(1, 0, 0) or Color3.new(1, 1, 1)
                end
            end
        end
    end
	end
})

