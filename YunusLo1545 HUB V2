tabFE:CreateToggle({
	Name = "FE God Mode",
	CurrentValue = false,
	Flag = "GodModeFE",
	Callback = function(Value)
		local plr = game.Players.LocalPlayer
local char = plr.Character or plr.CharacterAdded:Wait()
local hum = char:WaitForChild("Humanoid")

-- Karakter yeniden doğarsa güncelle
plr.CharacterAdded:Connect(function(c)
    char = c
    hum = char:WaitForChild("Humanoid")
end)

-- Canı sabitle ama GUI'de az görünsün
game:GetService("RunService").Heartbeat:Connect(function()
    if Value and hum and hum.Health > 0 then
        -- Gerçek canı koru
        hum.Health = hum.MaxHealth
        hum:SetStateEnabled(Enum.HumanoidStateType.Dead, false)

        -- GUI'de düşük görünmesi için sahte değer ver
        hum.HealthDisplayType = Enum.HumanoidHealthDisplayType.AlwaysOn
        hum.NameDisplayDistance = 0
        hum.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None
    end
end)
	end
})

tabFE:CreateToggle({
	Name = "Anti Kick / Ban",
	CurrentValue = false,
	Flag = "AntiKick",
	Callback = function(Value)
		antiKickEnabled = Value
    if Value then
        -- OnKick fonksiyonunu engelle
        originalOnKick = LocalPlayer.OnKick
        LocalPlayer.OnKick = function()
            -- Sonsuz bekleme ile kick'i engellemeye çalışır
            wait(9e9)
        end
    else
        -- Orijinal OnKick fonksiyonunu geri yükle
        if originalOnKick then
            LocalPlayer.OnKick = originalOnKick
            originalOnKick = nil
        else
            LocalPlayer.OnKick = nil
        end
    end
	end
})

