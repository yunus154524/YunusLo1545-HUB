-- Extras

tabClient:CreateInput({
	Name = "Teleport To Player",
	PlaceholderText = "Player Name",
	RemoveTextAfterFocusLost = false,
	Callback = function(Input)
		if Input == "" then return end
    local target = nil
    local linput = Input:lower()
    for _,plr in pairs(Players:GetPlayers()) do
        if plr.Name:lower():find(linput) then
            target = plr
            break
        end
    end
    if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
        rootPart.CFrame = target.Character.HumanoidRootPart.CFrame + Vector3.new(0,5,0)
    else
        warn("Oyuncu bulunamadı veya karakter hazır değil!")
    end
	end
})

local playerNames = {}
local function refreshPlayerNames()
    playerNames = {}
    for _,plr in pairs(Players:GetPlayers()) do
        table.insert(playerNames, plr.Name)
    end
end
refreshPlayerNames()

tabClient:CreateDropdown({
	Name = "Teleport To Player (Select)",
	Options = playerNames,
	CurrentOption = "",
	Flag = "TeleportDropdown",
	Callback = function(Option)
		local plr = Players:FindFirstChild(Option)
    if plr and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
        rootPart.CFrame = plr.Character.HumanoidRootPart.CFrame + Vector3.new(0,5,0)
    else
        warn("Oyuncu bulunamadı veya hazır değil!")
    end
	end
})

Players.PlayerAdded:Connect(refreshPlayerNames)
Players.PlayerRemoving:Connect(refreshPlayerNames)

tabClient:CreateToggle({
	Name = "Auto Rejoin",
	CurrentValue = false,
	Flag = "AutoRejoin",
	Callback = function(Value)
		autoRejoinEnabled = Value
    if Value then
        if autoRejoinConn then autoRejoinConn:Disconnect() end
        autoRejoinConn = LocalPlayer.OnTeleport:Connect(function()
            if autoRejoinEnabled then
                TeleportService:Teleport(game.PlaceId, LocalPlayer)
            end
        end)
    else
        if autoRejoinConn then autoRejoinConn:Disconnect() end
    end
	end
})

tabClient:CreateToggle({
	Name = "Anti AFK",
	CurrentValue = false,
	Flag = "AntiAFK",
	Callback = function(Value)
		antiAfkEnabled = Value
    if Value then
        antiAfkConn = game:GetService("VirtualUser")
        antiAfkConn:Button2Down(Vector2.new(0,0), Workspace.CurrentCamera.CFrame)
        antiAfkConn:Button2Up(Vector2.new(0,0), Workspace.CurrentCamera.CFrame)
    else
        antiAfkConn = nil
    end
	end
})

tabClient:CreateButton({
	Name = "Super Punch",
	Callback = function()
		for _, plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
            local dist = (plr.Character.HumanoidRootPart.Position - rootPart.Position).Magnitude
            if dist < 10 then
                local hitbox = Instance.new("Part")
                hitbox.Size = Vector3.new(10,10,10)
                hitbox.Transparency = 1
                hitbox.CanCollide = false
                hitbox.Anchored = true
                hitbox.CFrame = plr.Character.HumanoidRootPart.CFrame
                hitbox.Parent = Workspace
                game.Debris:AddItem(hitbox, 0.1)
            end
        end
    end
	end
})

tabClient:CreateToggle({
	Name = "Speed Aura",
	CurrentValue = false,
	Flag = "SpeedAura",
	Callback = function(Value)
		speedAuraEnabled = Value
    if speedAuraEnabled then
        speedAuraConn = RunService.Heartbeat:Connect(function()
            for _,plr in pairs(Players:GetPlayers()) do
                if plr ~= LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                    local dist = (plr.Character.HumanoidRootPart.Position - rootPart.Position).Magnitude
                    local hum = plr.Character:FindFirstChild("Humanoid")
                    if dist < 15 and hum then
                        hum.WalkSpeed = 5
                    elseif hum then
                        hum.WalkSpeed = 16
                    end
                end
            end
        end)
    else
        if speedAuraConn then speedAuraConn:Disconnect() end
        for _,plr in pairs(Players:GetPlayers()) do
            if plr.Character then
                local hum = plr.Character:FindFirstChild("Humanoid")
                if hum then
                    hum.WalkSpeed = 16
                end
            end
        end
    end
	end
})

tabClient:CreateToggle({
	Name = "Infinite Jump",
	CurrentValue = false,
	Flag = "InfJump",
	Callback = function(Value)
		infiniteJumpEnabled = Value
	end
})
UserInputService.JumpRequest:Connect(function()
    if infiniteJumpEnabled then
        if humanoid then
            humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end
end)

tabClient:CreateSlider({
	Name = "WalkSpeed Multiplier",
	Range = {1, 10},
	Increment = 1,
	Suffix = "x",
	CurrentValue = 1,
	Flag = "WalkMult",
	Callback = function(Value)
		walkSpeedMult = Value
    if humanoid then
        humanoid.WalkSpeed = 16 * walkSpeedMult
    end
	end
})

tabClient:CreateSlider({
	Name = "JumpPower Multiplier",
	Range = {1, 10},
	Increment = 1,
	Suffix = "x",
	CurrentValue = 1,
	Flag = "JumpMult",
	Callback = function(Value)
		jumpPowerMult = Value
    if humanoid then
        humanoid.JumpPower = 50 * jumpPowerMult
    end
	end
})

tabClient:CreateToggle({
	Name = "Chat Spam",
	CurrentValue = false,
	Flag = "ChatSpam",
	Callback = function(Value)
		spamEnabled = Value
    if spamEnabled then
        spamConn = RunService.Heartbeat:Connect(function()
            if TextChatService.ChatVersion == Enum.TextChatVersion.LegacyChatService then
                local defaultChatEvents = game:GetService("ReplicatedStorage"):FindFirstChild("DefaultChatSystemChatEvents")
                if defaultChatEvents then
                    local sayMessageRequest = defaultChatEvents:FindFirstChild("SayMessageRequest")
                    if sayMessageRequest then
                        sayMessageRequest:FireServer(spamMessages[math.random(1, #spamMessages)], "All")
                    end
                end
            elseif TextChatService.ChatVersion == Enum.TextChatVersion.TextChatService then
                local defaultChannel = TextChatService:FindFirstChild("TextChannels"):FindFirstChild("RBXGeneral")
                if defaultChannel then
                    defaultChannel:SendAsync(spamMessages[math.random(1, #spamMessages)])
                end
            end
        end)
    elseif spamConn then
        spamConn:Disconnect()
        spamConn = nil
    end
	end
})

local spamMessages = {
    "Solara rocks!", "I hacked this game!", "Epic Script!", "Join my Discord!", "Follow me!", "Best GUI ever!"
}
local TextChatService = game:GetService("TextChatService")

tabClient:CreateToggle({
	Name = "Teleport to Mouse (T toggle)",
	CurrentValue = false,
	Flag = "TeleportMouse",
	Callback = function(Value)
		teleportToMouseEnabled = Value
	end
})

UserInputService.InputBegan:Connect(function(input, gp)
    if gp then return end
    if input.KeyCode == Enum.KeyCode.T and teleportToMouseEnabled then
        local mouseLoc = UserInputService:GetMouseLocation()
        local ray = Workspace.CurrentCamera:ScreenPointToRay(mouseLoc.X, mouseLoc.Y)
        local pos = ray.Origin + ray.Direction * 200
        if rootPart then
            rootPart.CFrame = CFrame.new(pos)
        end
    end
end)

